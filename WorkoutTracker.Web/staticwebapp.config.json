{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "http://localhost:5050/api/$1"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/api/*"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-inline' 'unsafe-eval';",
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
} 